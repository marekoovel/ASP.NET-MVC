@model IEnumerable<Domain.Genre>


@{
    ViewBag.Title = "Store";
}

<h3>Browse Genres</h3>
<p>
    @Resources.Common.Select @Model.Count() @Resources.Common.Genres:
</p>

<ul>
    @foreach (var genre in Model)
    {
        <li>@Html.ActionLink(genre.Name, "Browse", new { genre = genre.Name })</li>
    }
</ul>
